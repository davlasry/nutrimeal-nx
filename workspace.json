{
  "version": 2,
  "projects": {
    "data": {
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/data/**/*.ts"]
          }
        }
      }
    },
    "nutrimeal": {
      "root": "apps/nutrimeal",
      "sourceRoot": "apps/nutrimeal/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nutrimeal",
            "index": "apps/nutrimeal/src/index.html",
            "main": "apps/nutrimeal/src/main.tsx",
            "polyfills": "apps/nutrimeal/src/polyfills.ts",
            "tsConfig": "apps/nutrimeal/tsconfig.app.json",
            "assets": [
              "apps/nutrimeal/src/favicon.ico",
              "apps/nutrimeal/src/assets"
            ],
            "styles": ["apps/nutrimeal/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/nutrimeal/src/environments/environment.ts",
                  "with": "apps/nutrimeal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "nutrimeal:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "nutrimeal:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nutrimeal/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nutrimeal"],
          "options": {
            "jestConfig": "apps/nutrimeal/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nutrimeal-api": {
      "root": "apps/nutrimeal-api",
      "sourceRoot": "apps/nutrimeal-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nutrimeal-api",
            "main": "apps/nutrimeal-api/src/main.ts",
            "tsConfig": "apps/nutrimeal-api/tsconfig.app.json",
            "assets": ["apps/nutrimeal-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nutrimeal-api/src/environments/environment.ts",
                  "with": "apps/nutrimeal-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "nutrimeal-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nutrimeal-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nutrimeal-api"],
          "options": {
            "jestConfig": "apps/nutrimeal-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nutrimeal-e2e": {
      "root": "apps/nutrimeal-e2e",
      "sourceRoot": "apps/nutrimeal-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nutrimeal-e2e/cypress.json",
            "tsConfig": "apps/nutrimeal-e2e/tsconfig.e2e.json",
            "devServerTarget": "nutrimeal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nutrimeal:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nutrimeal-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "nutrimeal"
}
